<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:b0="http://www.nrs.eskom.co.za/xmlvend/base/2.1/schema" xmlns:i0="http://www.nrs.eskom.co.za/xmlvend/revenue/2.1/schema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.nrs.eskom.co.za/xmlvend/revenue/2.1/schema" xmlns="http://www.w3.org/2001/XMLSchema">
  <import schemaLocation="xmlvend-base-2.1.xsd" namespace="http://www.nrs.eskom.co.za/xmlvend/base/2.1/schema" />
  <annotation>
    <documentation> Hand coded XML Schema for NRS Prepaid Utility Vending
			Service defining the
			revenue domain Vending Markup Language (XMLVend)
			for SOAP request and response messages
			[*** WS-I Compliant *** see
			http://www.ws-i.org]

			Version Number Date Namespace

			2.0 October 2005 http://www.nrs.eskom.co.za/XMLVend/revenue/2.0/schema
			2.1 February 2006
			http://www.nrs.eskom.co.za/XMLVend/revenue/2.1/schema
		</documentation>
  </annotation>
  <element name="confirmCustomerReq" type="i0:ConfirmCustomerReq" />
  <complexType name="ConfirmCustomerReq">
    <annotation>
      <documentation> A request to obtain customer details. </documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq">
        <sequence>
          <element name="idMethod" type="b0:IDMethod">
            <annotation>
              <documentation> The identification method used to search for a
								customer.</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="confirmCustomerResp" type="i0:ConfirmCustomerResp" />
  <complexType name="ConfirmCustomerResp">
    <annotation>
      <documentation> A response containing customer details.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element maxOccurs="unbounded" name="confirmCustResult" type="i0:ConfirmCustResult" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="creditVendReq" type="i0:CreditVendReq" />
  <complexType name="AbstractCreditVendReq" abstract="true">
    <annotation>
      <documentation> Abstract request to obtain a credit token. Must be
				specialised.</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendReq">
        <sequence>
          <element name="purchaseValue" type="i0:PurchaseValue" />
          <element minOccurs="0" name="payType" type="b0:PayType" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="CreditVendReq">
    <annotation>
      <documentation> Credit Vend request to obtain a credit token (normal
				sale).</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:AbstractCreditVendReq" />
    </complexContent>
  </complexType>
  <complexType name="TrialCreditVendReq">
    <annotation>
      <documentation> Trial Credit Vend requests a credit token (normal
				sale) from the server, without the server actually generating a
				valid token.</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:AbstractCreditVendReq" />
    </complexContent>
  </complexType>
  <element name="creditVendResp" type="i0:CreditVendResp" />
  <complexType name="CreditVendResp">
    <annotation>
      <documentation> A response to a creditVend request, containing the
				prepaid transaction
				and token cipher(possibly other transactions may
				be present such as, debt recovery).
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendResp">
        <sequence>
          <element name="creditVendReceipt" type="i0:CreditVendReceipt" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="fbeReq" type="i0:FBEReq" />
  <complexType name="FBEReq">
    <annotation>
      <documentation> A request for the free basic electricity token.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendReq" />
    </complexContent>
  </complexType>
  <element name="fbeResp" type="i0:FBEResp" />
  <complexType name="FBEResp">
    <annotation>
      <documentation> The response to a FBE request, containing the FBE
				token cipher.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:CreditVendResp" />
    </complexContent>
  </complexType>
  <element name="reprintReq" type="i0:ReprintReq" />
  <complexType name="ReprintReq">
    <annotation>
      <documentation> A request to obtain the reprint of transaction/s.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq">
        <sequence>
          <element name="idMethod" type="b0:IDMethod">
            <annotation>
              <documentation>
								Identification information used to search for the
								transactions on the server.
							</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="reprintResp" type="i0:ReprintResp" />
  <complexType name="ReprintResp">
    <annotation>
      <documentation> The response to a reprint request, containing one or
				more previous
				transactions for a specific meter.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element maxOccurs="unbounded" name="reprint" type="b0:BaseVendResp" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="cancelVendReq" type="i0:CancelVendReq" />
  <complexType name="CancelVendReq">
    <annotation>
      <documentation> A request to cancel a previously processed credit
				vend transaction -
				only applicable to mag card meters and reprint use
				case disabled.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendReq">
        <sequence>
          <element name="token" type="b0:Token" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="cancelVendResp" type="i0:CancelVendResp" />
  <complexType name="CancelVendResp">
    <annotation>
      <documentation> The response to cancel request, confirming that the
				transaction has been
				canceled and the original transaction.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendResp">
        <sequence>
          <element name="origTx" type="i0:CreditVendTx">
            <annotation>
              <documentation> The details of the transaction that was canceled.
							</documentation>
            </annotation>
          </element>
        </sequence>
        <attribute name="cancelRefNo" type="i0:ReceiptNo" use="optional">
          <annotation>
            <documentation> Identifier to the cancel vend transaction.
						</documentation>
          </annotation>
        </attribute>
      </extension>
    </complexContent>
  </complexType>
  <element name="validateCustomer" type="i0:ValidateCustomer" />
  <element name="postYakaPayment" type="i0:PostYakaPayment" />
  <element name="getTransactionDetails" type="i0:GetTransactionDetails" />
  <complexType name="ValidateCustomer">
    <sequence>
      <element name="customerRef" type="b0:CustomerRef" />
      <element name="vendorCode" type="b0:VendorCode" />
      <element name="password" type="b0:Password" />
    </sequence>
  </complexType>
  <complexType name="PostYakaPayment">
    <sequence>
      <element name="trans" type="i0:Trans" />
    </sequence>
  </complexType>
  <complexType name="Trans">
    <sequence>
      <element minOccurs="0" name="statusDescription" type="string" />
      <element minOccurs="0" name="statusCode" type="string" />
      <element minOccurs="0" name="digitalSignature" type="string" />
      <element minOccurs="0" name="paymentDate" type="string" />
      <element minOccurs="0" name="teller" type="string" />
      <element minOccurs="0" name="tranNarration" type="string" />
      <element minOccurs="0" name="tranIdToReverse" type="decimal" />
      <element minOccurs="0" name="paymentType" type="string" />
      <element minOccurs="0" name="tranType" type="string" />
      <element minOccurs="0" name="customerName" type="string" />
      <element minOccurs="0" name="customerType" type="string" />
      <element minOccurs="0" name="customerTel" type="string" />
      <element name="customerRef" type="b0:CustomerRef" />
      <element name="vendorTranId" type="b0:VendorTranId" />
      <element name="password" type="b0:Password" />
      <element name="vendorCode" type="b0:VendorCode" />
      <element name="tranAmount" type="decimal" />
    </sequence>
  </complexType>
  <complexType name="GetTransactionDetails">
    <sequence>
      <element name="vendorTranId" type="b0:VendorTranId" />
      <element name="vendorCode" type="b0:VendorCode" />
      <element name="password" type="b0:Password" />
    </sequence>
  </complexType>
  <element name="validateCustomerResponse" type="i0:ValidateCustomerResponse" />
  <element name="postYakaPaymentResponse" type="i0:PostYakaPaymentResponse" />
  <complexType name="ValidateCustomerResponse">
    <sequence>
      <element name="validateCustomerResult" type="i0:ValidateCustomerResult" />
    </sequence>
  </complexType>
  <complexType name="ValidateCustomerResult">
    <sequence>
      <element name="statusDescription" type="string" />
      <element name="statusCode" type="string" />
      <element name="customerRef" type="string" />
      <element name="customerName" type="string" />
      <element name="CustomerType" type="string" />
      <element name="balance" type="decimal" />
      <element name="custAddress" type="string" />
      <element name="custLocRef" type="string" />
      <element name="mobile" type="string" />
      <element name="vendorMaxVendAmt " type="decimal" />
      <element name="vendorMinVendAmt " type="decimal" />
    </sequence>
  </complexType>
  <complexType name="PostYakaPaymentResponse">
    <sequence>
      <element name="postYakaPaymentResult" type="i0:PostYakaPaymentResult" />
    </sequence>
  </complexType>
  <complexType name="PostYakaPaymentResult">
    <sequence>
      <element name="lifeLine" type="string" />
      <element name="payAccount" type="decimal" />
      <element name="remainingCredit" type="decimal" />
      <element name="debtRecovery" type="decimal" />
      <element name="arrearsBalance" type="decimal" />
      <element name="arrears" type="decimal" />
      <element name="receiptNumber" type="string" />
      <element name="statusCode" type="string" />
      <element name="statusDescription" type="string" />
      <element name="meterNumber" type="string" />
      <element name="units" type="decimal" />
      <element name="tokenValue" type="decimal" />
      <element name="tariffCodeNumber" type="string" />
      <element name="taxBase" type="decimal" />
      <element name="tax" type="decimal" />
      <element name="SSF" type="decimal" />
      <element name="maxDemandValue" type="decimal" />
      <element name="maxDemandCharge" type="decimal" />
      <element name="penalty" type="decimal" />
      <element name="reward" type="decimal" />
      <element name="prepaidToken" type="string" />
      <element name="accountPayAmount" type="decimal" />
      <element name="accountSaveAmount" type="decimal" />
      <element name="vendorTranId" type="string" />
      <element name="vendorMinVendAmt" type="decimal" />
      <element name="change" type="decimal" />
    </sequence>
  </complexType>
  <element name="trialCreditVendReq" type="i0:TrialCreditVendReq" />
  <element name="trialCreditVendResp" type="i0:TrialCreditVendResp" />
  <complexType name="TrialCreditVendResp">
    <annotation>
      <documentation>Response to trial vend request, same as a credit vend
				response, except
				that token returned has no monetary value and is set
				to "0000 0000 0000 0000".
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:CreditVendResp" />
    </complexContent>
  </complexType>
  <element name="meterCreditTransferReq" type="i0:MeterCreditTransferReq" />
  <complexType name="MeterCreditTransferReq">
    <annotation>
      <documentation> Request for a credit token in units, for credit left
				in a changed out
				meter.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendReq">
        <sequence>
          <element name="chgOutUnits" type="i0:PurchaseValueUnits">
            <annotation>
              <documentation> The requested value of the token, in terms of
								units.
							</documentation>
            </annotation>
          </element>
          <element name="creditTransferDetail" type="i0:CreditTransferDetail" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="meterCreditTransferResp" type="i0:MeterCreditTransferResp" />
  <complexType name="MeterCreditTransferResp">
    <annotation>
      <documentation> Response to a changeOutVend Request, contains the
				credit token, and the
				change out details.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendResp">
        <sequence>
          <element name="creditTransferDetail" type="i0:CreditTransferDetail" />
          <element name="creditTransferTx" type="i0:CreditVendTx" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="payAccReq" type="i0:PayAccReq" />
  <complexType name="PayAccReq">
    <annotation>
      <documentation> Request to pay an account. </documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq">
        <sequence>
          <element name="reqAmt" type="b0:Currency">
            <annotation>
              <documentation> The amount being paid. </documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="payType" type="b0:PayType" />
          <element name="payAccount" type="i0:PayAccount" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="payAccResp" type="i0:PayAccResp" />
  <complexType name="PayAccResp">
    <annotation>
      <documentation> Response, to a pay account request. Containing a
				receipt, confirming
				that the account has been paid.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendResp">
        <sequence>
          <element name="payAccReceipt" type="i0:PayAccReceipt" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="verifyReq" type="i0:VerifyReq" />
  <complexType name="VerifyReq">
    <annotation>
      <documentation> Request to verify an already issued credit token.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq">
        <sequence>
          <element name="token" type="b0:Token" />
          <element name="idMethod" type="b0:VendIDMethod">
            <annotation>
              <documentation> The meter identifier, for the meter whose token
								is being
								verified.
							</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="verifyResp" type="i0:VerifyResp" />
  <complexType name="VerifyResp">
    <annotation>
      <documentation> Response, to a verify token request. Contains details
				of the token being
				verified.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendResp">
        <sequence>
          <element name="creditVendTx" type="i0:CreditVendTx" />
          <element name="tokenInfo" type="b0:TokenInfo" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="freeIssueReq" type="i0:FreeIssueReq" />
  <complexType name="FreeIssueReq">
    <annotation>
      <documentation> Request for a credit token, that does not require
				payment and is issued
				for a specific purpose eg. marketing, etc.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseVendReq" />
    </complexContent>
  </complexType>
  <element name="freeIssueResp" type="i0:FreeIssueResp" />
  <complexType name="FreeIssueResp">
    <annotation>
      <documentation> Response to free issue request. Same as the credit
				vend response.
				Contains the credit token cipher and transaction
				details.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:CreditVendResp" />
    </complexContent>
  </complexType>
  <element name="startBatchReq" type="i0:StartBatchReq" />
  <complexType name="StartBatchReq" abstract="true">
    <annotation>
      <documentation> Request to explicitly start a banking, sales, shift
				or "all" on the
				server, before starting to vend.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq" />
    </complexContent>
  </complexType>
  <element name="startBatchResp" type="i0:StartBatchResp" />
  <complexType name="StartBatchResp">
    <annotation>
      <documentation> Response to start batch request. Confirms the batch
				or batches were
				started.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element name="startedBatch" type="i0:StartBatch" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="endBatchReq" type="i0:EndBatchReq" />
  <complexType name="EndBatchReq" abstract="true">
    <annotation>
      <documentation> Request to end a batch or batches on the server.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq" />
    </complexContent>
  </complexType>
  <element name="endBatchResp" type="i0:EndBatchResp" />
  <complexType name="EndBatchResp">
    <annotation>
      <documentation> Response to and end batch request. The batch or
				batches are closed and
				the summaries returned.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element name="endBatch" type="i0:Batch" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="totalBatchReq" type="i0:TotalBatchReq" />
  <complexType name="TotalBatchReq" abstract="true">
    <annotation>
      <documentation> Request to totalise the request batch or batches.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq" />
    </complexContent>
  </complexType>
  <element name="totalBatchResp" type="i0:TotalBatchResp" />
  <complexType name="TotalBatchResp">
    <annotation>
      <documentation> Response to totalise batch request. The batch
				summaries to date are
				returned. Batch status remains unchanged.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element maxOccurs="unbounded" name="totalBatch" type="i0:Batch" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="vendorStatementReq" type="i0:VendorStatementReq" />
  <complexType name="VendorStatementReq">
    <annotation>
      <documentation> Request to obtain last X number of vendor statement
				transactions of the
				vendors credit statement.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq" />
    </complexContent>
  </complexType>
  <element name="vendorStatementResp" type="i0:VendorStatementResp" />
  <complexType name="VendorStatementResp">
    <annotation>
      <documentation> Response to vendor statement request. Details last X
				vendor statement
				transactions on the vendors credit.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element name="statement" type="i0:VendorStatement" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="depositReq" type="i0:DepositReq" />
  <complexType name="DepositReq">
    <annotation>
      <documentation> Request to create a deposit slip for the vendor to
				bank his sales.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq" />
    </complexContent>
  </complexType>
  <element name="depositResp" type="i0:DepositResp" />
  <complexType name="DepositResp">
    <annotation>
      <documentation> Response to deposit request. Provides total sales
				since last deposit
				request and details to create a deposit slip.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element name="depSlip" type="i0:DepositSlip" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="reprintDepReq" type="i0:ReprintDepositReq" />
  <complexType name="ReprintDepositReq">
    <annotation>
      <documentation> Request to reprint an already requested deposit slip.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq" />
    </complexContent>
  </complexType>
  <element name="reprintDepResp" type="i0:ReprintDepositResp" />
  <complexType name="ReprintDepositResp">
    <annotation>
      <documentation> Response to reprint deposit slip request. Contains
				the last X created
				deposit slips.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element maxOccurs="unbounded" name="reprint" type="i0:DepositResp" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="reprintEndBatchReq" type="i0:ReprintEndBatchReq" />
  <complexType name="ReprintEndBatchReq" abstract="true">
    <annotation>
      <documentation> Abstract request to reprint an already closed batch
				or batches.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseReq" />
    </complexContent>
  </complexType>
  <complexType name="ReprintEndShiftBatchReq">
    <annotation>
      <documentation> Request to reprint an already closed shift batch, if
				the shift batch
				seqno is not supplied then the client's last closed
				shift batch is returned.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:ReprintEndBatchReq">
        <sequence>
          <element minOccurs="0" name="seqNo" type="i0:ShiftSalesBatchSeqNo" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="ReprintEndSalesBatchReq">
    <annotation>
      <documentation> Request to reprint an already closed sales batch and
				optionally any
				children shift batch / batches. If the sales batch
				seqno is not supplied then the
				client's last closed sales batch is
				returned.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:ReprintEndBatchReq">
        <sequence>
          <element minOccurs="0" name="seqNo" type="i0:ShiftSalesBatchSeqNo" />
          <element minOccurs="0" default="true" name="all" type="boolean" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="ReprintEndBankingBatchReq">
    <annotation>
      <documentation> Request to reprint an already closed banking batch
				and optionally any
				children sales and shift batch / batches. If the
				bank batch seqno is not supplied
				then the client's last closed sales
				batch is returned.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="i0:ReprintEndBatchReq">
        <sequence>
          <element minOccurs="0" name="seqNo" type="i0:BankBatchSeqNo" />
          <element minOccurs="0" default="true" name="all" type="boolean" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <element name="reprintEndBatchResp" type="i0:ReprintEndBatchResp" />
  <complexType name="ReprintEndBatchResp">
    <annotation>
      <documentation> Response to batch reprint request. Contains the
				requested batch.
			</documentation>
    </annotation>
    <complexContent mixed="false">
      <extension base="b0:BaseResp">
        <sequence>
          <element name="endBatchReprint" type="i0:EndBatchResp" />
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="Batch" abstract="true">
    <sequence>
      <element name="started" type="dateTime">
        <annotation>
          <documentation> Date time stamped of when the batch was started.
					</documentation>
        </annotation>
      </element>
      <element name="ended" type="dateTime">
        <annotation>
          <documentation> Date time stamped of when the batch was closed.
					</documentation>
        </annotation>
      </element>
      <element name="batchTot" type="b0:Currency" />
      <element minOccurs="0" name="totTax" type="b0:Tax" />
